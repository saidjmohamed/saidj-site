{
  "name": "saidj-law-office",
  "version": "2.1.0",
  "description": "Modern Law Office Website - موقع مكتب محاماة عصري للأستاذ سعيد محمد",
  "keywords": [
    "law-office",
    "lawyer",
    "legal-services",
    "algeria",
    "محاماة",
    "قانون",
    "استشارات-قانونية"
  ],
  "homepage": "https://saidjlawyer.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/saidjmohamed/saidj-site.git"
  },
  "author": {
    "name": "الأستاذ سعيد محمد",
    "email": "contact@saidjlawyer.com",
    "url": "https://saidjlawyer.vercel.app"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "analyze": "npx vite-bundle-analyzer",
    "clean": "rm -rf dist node_modules/.vite",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest --coverage",
    "deploy": "npm run build && vercel --prod",
    "pre-commit": "npm run lint:fix && npm run format && npm run type-check"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "framer-motion": "^10.16.4",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "date-fns": "^2.30.0",
    "clsx": "^2.0.0",
    "react-intersection-observer": "^9.5.2",
    "react-hot-toast": "^2.4.1",
    "react-hook-form": "^7.47.0",
    "zod": "^3.22.4",
    "@hookform/resolvers": "^3.3.2",
    "lucide-react": "^0.294.0",
    "react-router-dom": "^6.18.0",
    "@tanstack/react-query": "^5.8.4",
    "axios": "^1.6.1",
    "js-cookie": "^3.0.5",
    "react-helmet-async": "^1.3.0",
    "react-lazyload": "^3.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/js-cookie": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.4",
    "vite-plugin-windicss": "^1.9.3",
    "windicss": "^3.5.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "jsdom": "^23.0.1",
    "vite-bundle-analyzer": "^0.7.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "vite-plugin-eslint": "^1.8.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "eslintConfig": {
    "extends": [
      "@typescript-eslint/recommended",
      "plugin:react-hooks/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "react-refresh"
    ],
    "rules": {
      "react-refresh/only-export-components": [
        "warn",
        { "allowConstantExport": true }
      ],
      "@typescript-eslint/no-unused-vars": "warn",
      "@typescript-eslint/no-explicit-any": "warn"
    },
    "ignorePatterns": ["dist", ".eslintrc.cjs"]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}